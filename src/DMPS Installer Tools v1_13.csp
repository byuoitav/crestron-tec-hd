FUNCTION CalculateScrollBar(INTEGER Total, INTEGER Index)
	{
	if(Index > 1)
		DigitalOut[1] = 1;
	else
		DigitalOut[1] = 0;
	if(Index < Total)
		DigitalOut[2] = 1;
	else
		DigitalOut[2] = 0;
	}

FUNCTION CalculateLineScrollBar(INTEGER Total, INTEGER Index)
	{
	if ( (Index = 1) && (Total = 1) )
		Line2ScrollArrow$ = ARROWNONE;
	else if(Index = 1)
		Line2ScrollArrow$ = ARROWDOWN;
	else if(Index = Total)
		Line2ScrollArrow$ = ARROWUP;
	else
		Line2ScrollArrow$ = ARROWUPDOWN;
	}

Function UpdateTime()
{
	if(temphour > 23)
	{
		temphour = 12;
	}

	if(temphour < 12)
 	{
  		if(temphour <> 0)
		{
			displayHour = temphour;
     	}
		else
		{
       		displayHour = 12;
     	}
  	}
	else
	{
	   	if(temphour <> 12)
		{
       		displayhour = temphour - 12;
     	}
    	else
		{
       		displayHour = 12;
     	}
  	}
}

